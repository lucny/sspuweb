        /* <![CDATA[ */
        function addMSMTIntranetOnLoad() {
          var oldonload = window.onload;
          
          if (typeof window.onload != 'function') {
            window.onload = function() {
              showMSMTIntranet();
            }
          } else {
            window.onload = function() {
              oldonload();
              showMSMTIntranet();
            }
          }
        }
        addMSMTIntranetOnLoad();
        /* ]]> */
    </script>
